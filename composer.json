    "name":         "jsvrcek/ics-bundle",
    "description":  "This bundle provides a dependency injection wrapper for the JsvrcekICS iCal library",
    "keywords":     ["ical", "calendar", "export", ".ics", "RFC 5545"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "homepage": "https://github.com/jasvrcek/IcsBundle",
    "authors":  [
        {
            "name": "Justin Svrcek",
            "homepage": "https://github.com/jasvrcek"
        }
    ],
    "require":      {
        "jsvrcek/ics":         	    "dev-master",
        "symfony/framework-bundle": ">=2.0"
    },
    "autoload":     {
        "psr-0": { "Jsvrcek\\Bundle\\IcsBundle": "" }
    },
    "target-dir": "Jsvrcek/Bundle/IcsBundle"
}
